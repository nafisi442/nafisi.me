                <!-- Resume -->
                <section id="resume">
                	 <div class="timeline-section">
                        <!-- Timeline for Employment  -->   
                        <h3 class="main-heading">Employment</h3>   
                        <ul class="timeline">
                        @foreach(nafisi.me.core.job job in nafisi.me.core.core.jobs_list){
                            <li>
                                                   
                                <div class="timelineUnit">
                                <h3>@job.company</h3>
                                    <h4>@job.title<span class="timelineDate">@job.from.ToString("MMMM yyyy") - @job.to.ToString("MMMM yyyy")</span></h4>
                                    <h5>@job.location</h5>
                                    <ul class="job_desc">
                                    @foreach (string description in job.descriptions)
                                    {
                                    <li>@description</li>
                                    }
                                    </ul>
                                </div>
                            </li>
                            
                        }
                            <div class="clear"></div>
                        </ul> 
                        <!-- /Timeline for Employment  -->

                        <!-- Timeline for Education  -->   
                        <h3 class="main-heading">Education</h3>   
                         <ul class="timeline">

                        @foreach (nafisi.me.core.eduction eduction in nafisi.me.core.core.education_list)
                        {
                            
                                                        <li>            
                                <div class="timelineUnit">
                                    <h4>@eduction.program<span class="timelineDate">@eduction.from.ToString("MMMM yyyy") - @eduction.to.ToString("MMMM yyyy")</span></h4>
                                    <h5>@eduction.school</h5>
                                     <h5>@eduction.location</h5>
                                </div>
                            </li>

                            
                        }


                            <div class="clear"></div>
                        </ul> 
                        <!-- /Timeline for Education  -->       
                        
                        
                                                <h3 class="main-heading">Work Authorization</h3>   
                         <ul class="timeline">
                            <li>            
                                <div class="timelineUnit">
                                    <h4>@nafisi.me.core.core.work_authorization</h4>
                                </div>
                            </li>
                            <div class="clear"></div>
                        </ul> 
                        
                               
                    </div>
                    <div class="skills-section">
                        <!-- Skills -->
                        <h3 class="main-heading">Skills Rating</h3> 
                        <ul class="skills">
                        @foreach (nafisi.me.core.technology technology in nafisi.me.core.core.technology_list.OrderByDescending(x => x.power))
                        {
                                  <li>
                                <h4>@technology.technology_name</h4>
                                <div class="rating" data-rat="@technology.power"></div>

                            </li>
                        }
                    

                        </ul>
                   
       

                     <!-- /Skills -->
                     </div>
